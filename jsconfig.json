{
  "include": "src",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@/lib/*": ["lib/*"],
      "@/api/*": ["lib/api/*"],
      "@/router/*": ["lib/api/router/*"],
      "@/helpers/*": ["lib/helpers/*"],
      "@/internals/*": ["components/main-layout/internals/*"],
      "@/common/*": ["components/main-layout/common/*"],
      "@/features/*": ["lib/features/*"]
    }
  }
}
